#include <stdio.h>
#include <conio.h>
main()
{
      int matriz[100][100];
      int i,j,cont;
      cont = 0;
      for (i=0; i<100; i++)
          for(j=0; j<100; j++)
          {
                   matriz[i][j] = cont;
                   printf("%d ", matriz[i][j]);
          }  /*A matriz atÃ© aqui recebeu o valor 0 em todos os pontos */
      printf("\n\nAPERTE <Enter> PARA PRENCHER DE 1 a 10000 A MATRIZ!\n\n");
      getch();
      cont = 1;
      for (i=0; i<100; i++)
      for (j=0; j<100; j++)
      {
          matriz[i][j] = cont;
          printf("%d ", matriz[i][j]);
          cont++;
      }     /* A matriz aqui recebeu de 1 a 10000 */
      getch();
}
